# Maintainer: ${author} <${author_email}>

pkgname=fman
pkgver=${version}
pkgrel=1
pkgdesc="${description}"
arch=('x86_64')
url="https://fman.io"
license=('custom')
depends=(${deps})
optdepends=(${opt_deps})
install=.INSTALL

source=('http://download.fman.io/fman.pkg.tar.xz')

sha256sums=('${sha256}')

package() {
	tar xf "${srcdir}/fman.pkg.tar.xz" -C "${pkgdir}"
}